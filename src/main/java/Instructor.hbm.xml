<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 5.6//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-5.6.dtd">
<hibernate-mapping>
    <class name="com.codegnan.entity.Instructor" table="instructor_101">
        <id name="id">
            <generator class = "increment"/>
        </id>

        <!-- Example property mapping -->
        <!-- <property name="propertyName" column="column_name" type="data_type"/> -->
        <property name = "first_name"/>
        <property name = "last_name"/>
        <property name = "email"/>
 
    </class>
</hibernate-mapping>